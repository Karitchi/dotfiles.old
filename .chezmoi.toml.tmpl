{{- $email := promptStringOnce . "email" "Email address" -}}
{{- $username := promptStringOnce . "username" "Username" -}}
{{- $text-editor := promptStringOnce . "text-editor" "Text editor" -}}

[git]
	autoCommit = true
	autoPush = true

[data]
	email = {{ $email | quote}}
	username = {{ $username | quote }}
	text-editor = {{ $text-editor | quote }}
